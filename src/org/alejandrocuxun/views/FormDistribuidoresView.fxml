        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnGuardar" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="80.0" prefWidth="117.0" text="Guardar" />
            <Button fx:id="btnCancelar" layoutX="10.0" layoutY="28.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="80.0" prefWidth="117.0" text="Cancelar" GridPane.columnIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
